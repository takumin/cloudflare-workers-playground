{
	"name": "cloudflare-workers-playground",
	"version": "0.0.0",
	"private": true,
	"packageManager": "pnpm@9.14.4",
	"engines": {
		"node": "^22.0.0 || ^20.0.0 || ^18.0.0"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"install": "aqua install",
		"postinstall": "lefthook install",
		"fmt": "biome check --write",
		"lint": "biome check --write",
		"test": "vitest",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"deploy": "wrangler deploy",
		"destroy": "wrangler delete --force",
		"typegen": "wrangler types",
		"update": "aqua update && pnpm update --latest"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "0.5.33",
		"@cloudflare/workers-types": "4.20241202.0",
		"typescript": "5.7.2",
		"vitest": "2.1.8",
		"wrangler": "3.92.0"
	}
}
